   gravimetricInsolubleResidue Double
   dateTime UTCTime
   samplesId SamplesId
   deriving Show
   
SampleType json
   description Text //deve armazenar se é parcial, geral, compostas ou talho - escolher um nome melhor e mais significativo
   deriving Show

Samples json
   resultTime UTCTime 
   sampleType SampleType //deve armazenar se é parcial de duas mil, dez mil ou algum tipo de composta - escolher um nome melhor e mais significativo
   deriving Show

UserType json
   userTypeDescription Text
   userTypeAccessLevel Int
   deriving Show

